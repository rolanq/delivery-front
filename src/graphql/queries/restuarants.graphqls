query GetRestaurants {
  getRestaurants {
    id
    name
    rating
    categories
    image
  }
}

query GetRestuarant($id: ID!) {
  getRestuarant(id: $id) {
    id
    name
    rating
    categories
    address
    description
    startWorkingDay
    endWorkingDay
    image
    MenuCategories {
      id
      name 
      MenuItems {
        id
        name
        image
        description
        weight
        price
      }
    }
  }
}
